{
  "name": "Fogo Dev Container",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": ".."
  },
  "postCreateCommand": "pip install -e .",
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python"
  },
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python"]
    }
  }
}